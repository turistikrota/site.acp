{
  "rules": {
    "no-undef": "off",
    "react/prop-types": "off"
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "$": true
  },
  "settings": {
    "react": {
      "version": "^17.0.1"
    }
  }
}
